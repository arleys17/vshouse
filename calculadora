import math
#colocar o avlor
valor=float(input('digite um numero:'))
#agora os multiplos
dobro=valor * 2
triplo= valor *3
quadrado=valor**2
raiz_quadrada= math.sqrt(valor)
raiz_cubica=valor**(1/3) 
# agora resultados
print(f'o dobro do valor e:{dobro}')
print(f'o triplo do valor e: {triplo}')
print(f'o quadrado do valor e :{quadrado}')
print(f'a raiz quadrada do valor e:{raiz_quadrada: .1f}')
print(f'a raiz cubica do valor e:{raiz_cubica: .1f}')
